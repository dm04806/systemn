


(p \/ q) /\ (~p \/ q) /\ (p \/ ~q) ==> ~(~q \/ ~q)

(q ==> r) /\ (r ==> p /\ q) /\ (p ==> q /\ r) ==> (p <=> q)

p <=> p

((p <=> q) <=> r) <=> (p <=> (q <=> r))

p \/ q /\ r <=> (p \/ q) /\ (p \/ r)

(p <=> q) <=> (q \/ ~p) /\ (~q \/ p)

p ==> q <=> ~p \/ q

(p ==> q) \/ (q ==> p)

p /\ (q ==> r) ==> s <=> (~p \/ q \/ s) /\ (~p \/ ~r \/ s)